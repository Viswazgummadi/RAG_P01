        {
          "type": "text",
          "content": "Measurement\nof Works of River Valley Projects\nSectional\nCommittee,\nRVD 23",
          "bbox": [
            15.62,
            27.43,
            381.86,
            40.23
          ],
          "block_id": "b2_0",

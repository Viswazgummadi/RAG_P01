      "blocks": [
        {
          "type": "text",
          "content": "ISO 1oo11-3:199I(E)",
          "bbox": [
            429.92,
            5.61,
            523.07,
            18.83
          ],
          "block_id": "b3_0",
          "font_size": 9.6,
          "is_bold": true,

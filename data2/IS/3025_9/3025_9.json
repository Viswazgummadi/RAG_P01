          "is_bold": true,
          "is_heading": true
        },
        {
          "type": "text",
          "content": "Z\n=  dep\u2019th in metres;",
          "bbox": [
            113.81,
            129.94,
            216.53,
            140.56
          ],
          "block_id": "b2_4",

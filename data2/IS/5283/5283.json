        {
          "type": "text",
          "content": "FIG. 1\nTYPICAL STORAGE-CISTERN-FED\nWATERER",
          "bbox": [
            70.08,
            363.9,
            304.62,
            375.43
          ],
          "block_id": "b6_5",
          "font_size": 8.6,
          "is_bold": true,
          "is_heading": true
        },
        {
          "type": "text",
          "content": "-3.3.2  Mains-Fed\nWaterer",
          "bbox": [
            27.6,
            381.69,

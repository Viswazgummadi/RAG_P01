          "font_size": 8.6,
          "is_bold": false,
          "is_heading": false
        },
        {
          "type": "text",
          "content": "3.3  Units of Measurement",
          "bbox": [
            252.58,
            280.54,
            359.27,
            292.07
          ],
          "block_id": "b3_22",
          "font_size": 8.6,
          "is_bold": true,
